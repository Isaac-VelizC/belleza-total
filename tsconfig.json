{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@containers/*": ["src/containers/*"],
      "@interface/*": ["src/interface/*"],
      "@lib/*": [
        "src/lib/*"
      ],
    }
  }
}
